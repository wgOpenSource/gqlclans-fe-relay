{
  "name": "gqlclans-react-relay",
  "version": "0.0.1",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-relay": "^1.4.1",
    "react-scripts": "1.1.0",
    "relay-runtime": "^1.4.1"
  },
  "scripts": {
    "setup": "node src/setup",
    "start": "yarn run setup && react-scripts start",
    "build": "yarn run setup && react-scripts build",
    "test": "yarn run setup && react-scripts test --env=jsdom",
    "fetch_schema": "node scripts/fetch_schema",
    "relay": "relay-compiler --src ./src --schema ./schema.json",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.4.1",
    "relay-compiler": "^1.4.1"
  }
}
